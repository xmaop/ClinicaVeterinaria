<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>.: Veterinaria PetCenter :.</title>
    <script type="text/javascript">var baseUrl = "@Url.Content("~")"; </script>
    @System.Web.Optimization.Scripts.Render("~/bundles/JQueryJs")
    <style type="text/css">
        /** no copiar por que ya lo tienes*/

        html, body {
            width: 100%;
            height: 100%;
            padding: 0;
            margin: 0;
        }

        body {
            font-size: 13px;
            font-family: Arial, Tahoma, Helvetica;
            margin: 0px;
            padding: 0px;
            background: #FFF;
            color: #555;
            line-height: 18px;
            min-width: 1002px;
        }

        ul {
            margin: 0px;
            padding: 0px;
        }

        li {
            list-style-type: none;
            padding: 0px;
            margin: 0px;
        }

        p {
            margin: 0px;
            padding: 0px;
        }

        img {
            margin: 0px;
            padding: 0px;
            vertical-align: top;
            border: 0px;
        }

        input, textarea {
            padding: 0px;
            margin: 0px;
            outline: 0 none;
            font-family: Arial, Helvetica;
            color: #666;
        }

        span {
            padding: 0px;
            margin: 0px;
        }

        h1, .h1 {
            font-size: 36px;
        }

        h2, .h2 {
            font-size: 30px;
        }

        h3, .h3 {
            font-size: 24px;
        }

        h4, .h4 {
            font-size: 18px;
        }

        h5, .h5 {
            font-size: 14px;
        }

        h6, .h6 {
            font-size: 12px;
        }

        .header_sesion {
            position: relative;
            z-index: 100;
            min-height: 52px;
            line-height: 52px;
            font-size: 17px;
            background: #0066b1;
            border-top: 2px solid #00599f;
            border-left: 1px solid #01559b;
            border-right: 1px solid #007cce;
            color: white;
            text-align: center;
            -webkit-box-shadow: 0px 1px 5px 0px rgba(50, 50, 50, 0.40);
            -moz-box-shadow: 0px 1px 5px 0px rgba(50, 50, 50, 0.40);
            box-shadow: 0px 1px 5px 0px rgba(50, 50, 50, 0.40);
        }

        .wrapper_center_sesion {
            text-align: center;
            border-bottom: 50px solid #F9F9F9;
            padding: 0px 0px 50px 0px;
            position: relative;
        }

            .wrapper_center_sesion:before {
                content: '';
                background: #e4e6e5;
                height: 1px;
                position: absolute;
                bottom: 0px;
                left: 0px;
                right: 0px;
            }

        .ico_sesion {
            text-align: center;
            padding: 40px 0px 40px 0px;
            /* Permalink - use to edit and share this gradient: http://colorzilla.com/gradient-editor/#f9f9f9+0,ffffff+100 */
            background: #f9f9f9; /* Old browsers */
            background: -moz-linear-gradient(top, #f9f9f9 0%, #ffffff 100%); /* FF3.6-15 */
            background: -webkit-linear-gradient(top, #f9f9f9 0%,#ffffff 100%); /* Chrome10-25,Safari5.1-6 */
            background: linear-gradient(to bottom, #f9f9f9 0%,#ffffff 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
            filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f9f9f9', endColorstr='#ffffff',GradientType=0 ); /* IE6-9 */
        }

        .btn_sesion {
            width: 80px;
            color: white;
            text-align: center;
            height: 33px;
            line-height: 33px;
            text-decoration: none;
            border: 1px solid #014691;
            padding: 0px 3px 0px 9px;
            margin: 8px 0px 0px 10px;
            position: relative;
            border-radius: 2px;
            background: #1c74cd; /* Old browsers */
            background: -moz-linear-gradient(top, #1c74cd 0%, #0061a8 100%); /* FF3.6+ */
            background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#1c74cd), color-stop(100%,#0061a8)); /* Chrome,Safari4+ */
            background: -webkit-linear-gradient(top, #1c74cd 0%,#0061a8 100%); /* Chrome10+,Safari5.1+ */
            background: -o-linear-gradient(top, #1c74cd 0%,#0061a8 100%); /* Opera 11.10+ */
            background: -ms-linear-gradient(top, #1c74cd 0%,#0061a8 100%); /* IE10+ */
            background: linear-gradient(to bottom, #1c74cd 0%,#0061a8 100%); /* W3C */
            filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#1c74cd', endColorstr='#0061a8',GradientType=0 ); /* IE6-9 */
            display: inline-block;
            *display: inline;
            zoom: 1;
        }

            .btn_sesion:hover {
                background: #0261a5; /* Old browsers */
                background: -moz-linear-gradient(top, #0261a5 0%, #1e73d0 100%); /* FF3.6+ */
                background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#0261a5), color-stop(100%,#1e73d0)); /* Chrome,Safari4+ */
                background: -webkit-linear-gradient(top, #0261a5 0%,#1e73d0 100%); /* Chrome10+,Safari5.1+ */
                background: -o-linear-gradient(top, #0261a5 0%,#1e73d0 100%); /* Opera 11.10+ */
                background: -ms-linear-gradient(top, #0261a5 0%,#1e73d0 100%); /* IE10+ */
                background: linear-gradient(to bottom, #0261a5 0%,#1e73d0 100%); /* W3C */
                filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#0261a5', endColorstr='#1e73d0',GradientType=0 ); /* IE6-9 */
            }
    </style>
</head>
<body class="bg">
    @RenderSection("featured", required: false)
    @RenderBody()
</body>
</html>
